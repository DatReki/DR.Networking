<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>DatReki</Authors>
    <Description>A small networking library to make get &amp; post requests</Description>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageReleaseNotes>Fixed readme and set to release mode (seems I was targeting wrong project before)</PackageReleaseNotes>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<PackageTags>http https IPv4 DNS networking request get post</PackageTags>
	<RepositoryType>git</RepositoryType>
	<RepositoryUrl>https://github.com/DatReki/DR.Networking</RepositoryUrl>
	<Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
	<None Include="README.md" Pack="true" PackagePath="\" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Nager.PublicSuffix" Version="2.2.2" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
